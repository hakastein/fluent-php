{
    "name": "jrmajor/fluent",
    "description": "Fluent localization system for PHP",
    "keywords": [
        "translator",
        "translation",
        "i18n",
        "internationalization",
        "l10n",
        "localization",
        "locale",
        "language",
        "ast",
        "parser",
        "plural",
        "gender",
        "ftl",
        "mozilla"
    ],
    "homepage": "https://github.com/jrmajor/fluent-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "jrmajor/pluralrules": "^1.0"
    },
    "require-dev": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "jrmajor/cs": "^0.3.2",
        "nunomaduro/collision": "^5.10",
        "phpstan/phpstan": "^1.1",
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/var-dumper": "^5.3",
        "symfony/console": "^5.3",
        "azjezz/psl": "^1.9",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Major\\Fluent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\Fluent\\Dev\\": "dev",
            "Major\\Fluent\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
