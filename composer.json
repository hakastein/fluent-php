{
    "name": "jrmajor/fluent",
    "description": "Fluent localization system for PHP",
    "keywords": [
        "localization",
        "l10n",
        "internationalization",
        "i18n",
        "ftl",
        "plural",
        "gender",
        "locale",
        "language",
        "formatting",
        "translate",
        "translation",
        "format",
        "ast",
        "serializer",
        "parser"
    ],
    "homepage": "https://github.com/jrmajor/fluent-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "jrmajor/pluralrules": "^1.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpstan/phpstan": "^0.12.99",
        "pestphp/pest": "^1.20",
        "phpunit/phpunit": "^9.5",
        "jrmajor/cs": "^0.3.1",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Major\\Fluent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\Fluent\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
